  "authors": [
    "RedLime"
  ],
  "contributors": [
    "snessim",
    "Heamaci"
  ],
  "contact": {
    "sources": "https://github.com/RedLime/DetailArmorBar/tree/1.19"
  },
  "license": "MIT",
  "icon": "assets/detailab/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      {
        "adapter": "default",
        "value": "com.redlimerl.detailab.DetailArmorBar"
      }
    ],
    "modmenu": [
      "com.redlimerl.detailab.config.ModMenuConfigImpl"
    ]
  },
  "mixins": [
    "detailab.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.11.6",
    "fabric": "*",
    "minecraft": ">=1.21 <1.22-",
